<Window x:Class="VostGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VostGame"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="550" KeyDown="pressKey">
    <Grid>
        <Canvas Name="Field" HorizontalAlignment="Center" VerticalAlignment="Top" Height="400" Width="500" Margin="0 50 0 0" Background="LightBlue"/>
        <TextBlock Text="Время(с): " HorizontalAlignment="Left" VerticalAlignment="Top" Margin="25 25 0 0"/>
        <TextBlock Name="TBTime" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="76 25 0 0"/>
    </Grid>
</Window>
